{
  "name": "parking-widget",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "sh bin/build.sh",
    "test": "ng test",
    "lint": "ng lint",
    "compodoc": "compodoc -p projects/widget/tsconfig.lib.json --theme material --output docs",
    "extract-i18n": "ngx-translate-extract --input projects/widget/src --output projects/widget/src/lib/assets/i18n/strings.pot --clean --sort --format pot",
    "e2e": "ng e2e"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "~8.2.14",
    "@angular/material": "~8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "hammerjs": "^2.0.8",
    "rxjs": "^6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-builders/jest": "^8.3.2",
    "@angular-devkit/build-angular": "^0.803.25",
    "@angular-devkit/build-ng-packagr": "^0.803.25",
    "@angular/cli": "^8.3.25",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@biesbjerg/ngx-translate-extract": "^4.2.0",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jest": "^24.9.0",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.2.1",
    "commitizen": "^4.0.3",
    "coveralls": "^3.0.9",
    "cz-conventional-changelog": "^3.0.2",
    "husky": "^4.0.10",
    "jest": "^24.9.0",
    "jest-html-reporter": "^2.8.0",
    "ng-packagr": "^5.7.1",
    "ngx-translate-testing": "^3.0.0",
    "protractor": "~5.4.0",
    "ts-jest": "^24.3.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  }
}
